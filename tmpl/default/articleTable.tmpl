<tr>
	<td class="digiseller-table-left">
		<a onclick="location.hash='<?= url ?>'"><?= d.name ?>                      
	</td>
	<td class="digiseller-table-right">
		<div>
			<span><? if (d.collection == 'unit') { ?> от <? } ?> <?= d.price ?> <?= d.currency ?></span>
			<? if (d.is_available) { ?>
				<a class="digiseller-buyButton" data-action="buy" data-id="<?= d.id ?>" data-ai="<?= d.agency_id ?>">Купить</a>
			<? } else { ?>
				<a class="digiseller-buyButton-empty">Нет в наличии</a>
			<? } ?>
		</div>
	</td>
</tr>